<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_map_show"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.liner.graduationproject.map.activity.MapNaviShowActivity">

    <com.amap.api.navi.view.OverviewButtonView
        android:id="@+id/activity_show_overview_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.amap.api.navi.AMapNaviView
        android:id="@+id/activity_show_basic_map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
